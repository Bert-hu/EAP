<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <log4net>
    <appender name="DebugLog" type="log4net.Appender.RollingFileAppender" >
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="Logs/Debug/" />
      <datePattern value="yyyyMMdd'.log'"/>
      <param name="Encoding" value="utf-8" />
      <appendToFile value="true" />
      <!--<rollingStyle value="Date" />-->
      <staticLogFileName value="false"/>
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="20"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level - %message%newline" />
      </layout>
    </appender>
    <appender name="TraceLog" type="EAP.Client.Forms.RichTextBoxAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="Logs/Trace/" />
      <datePattern value="yyyyMMdd'.log'"/>
      <param name="Encoding" value="utf-8" />
      <appendToFile value="true" />
      <!--<rollingStyle value="Date" />-->
      <staticLogFileName value="false"/>
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="20"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level - %message%newline" />
      </layout>
    </appender>
    <appender name="SecsLog" type="log4net.Appender.RollingFileAppender" >
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="Logs/Secs/" />
      <datePattern value="yyyyMMdd'.log'"/>
      <param name="Encoding" value="utf-8" />
      <appendToFile value="true" />
      <!--<rollingStyle value="Date" />-->
      <staticLogFileName value="false"/>
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="20"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date %newline%message%newline" />
      </layout>
    </appender>
    <appender name="NonSecsLog" type="log4net.Appender.RollingFileAppender" >
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="Logs/NonSecs/" />
      <datePattern value="yyyyMMdd'.log'"/>
      <param name="Encoding" value="utf-8" />
      <appendToFile value="true" />
      <!--<rollingStyle value="Date" />-->
      <staticLogFileName value="false"/>
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="20"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date %newline%message%newline" />
      </layout>
    </appender>
    <root>
      <!--<appender-ref ref="RollingFileAppender" />-->
    </root>
    <logger name="Debug">
      <appender-ref ref="DebugLog" />
    </logger>
    <logger name="Trace">
      <appender-ref ref="TraceLog" />
    </logger>
    <logger name="Secs">
      <appender-ref ref="SecsLog" />
    </logger>
    <logger name="NonSecs">
      <appender-ref ref="NonSecsLog" />
    </logger>
  </log4net>
</configuration>